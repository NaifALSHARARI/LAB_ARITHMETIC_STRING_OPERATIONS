price = 2.99
quantity = 3
tax_rate = 0.075
subtotal = price*quantity
tax = round(subtotal*tax_rate, 2)
Total = subtotal+tax
print ("Price of item:" , price)
print ("Quantity: " , quantity)
print (f"Tax rate: {tax_rate*100}%")
print ("Subtotal: " , subtotal)
print ("tax: " ,  tax)
print ("total : " , Total)